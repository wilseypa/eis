--synthesis_resources = lut 13 
SUBDESIGN add_sub_bmg
( 
	clock	:	input;
	dataa[5..0]	:	input;
	datab[5..0]	:	input;
	result[5..0]	:	output;
) 
VARIABLE
	pipeline_dffe[11..0]	:	DFFE
		WITH (
			power_up ="low"
		);
	result_int[5..0]	:	WIRE;
BEGIN 
	result_int[] = dataa[] - datab[];
	pipeline_dffe[].clk = clock;
	result[] = pipeline_dffe[11..6].q;
	pipeline_dffe[5..0].d = result_int[];
	pipeline_dffe[11..6].d = pipeline_dffe[5..0].q;
END;
